// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement()) @map("USER_PRIMARY_KEY")
  email     String @unique
  name      String
  device_id Int    @unique
}

model Order {
  id   Int      @id @default(autoincrement()) @map("ORDER_PRIMARY_KEY")
  type String
  date DateTime
}
